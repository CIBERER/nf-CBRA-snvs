    test("validate_automap - vcf") {

        when {
            process {
                """                
                input[0] = Channel.of([
                    [ id:'test'], // meta map
                    "PASS",
                    file("https://zenodo.org/records/15355673/files/Sample1_T1.gatk.PASS.vcf.gz?download=1")
                ])
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
                //TODO nf-core: Add all required assertions to verify the test output.
